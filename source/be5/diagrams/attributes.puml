   entity "resources" as resources{
     --
     *ID : KEYTYPE
     type : ENUM('database','other','program','recommendation')
     title : VARCHAR(20)
     version : VARCHAR(20)
     description : TEXT
     url : VARCHAR(100)
     license : VARCHAR(200)
     comment : TEXT
     creationDate___ : TIMESTAMP
     modificationDate___ : TIMESTAMP
     whoInserted___ : VARCHAR(100)
     whoModified___ : VARCHAR(100)
    }

